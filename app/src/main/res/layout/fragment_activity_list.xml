        android:id="@+id/recyclerview_activity_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/colorWhite"
        android:scrollbars="vertical"/>
</FrameLayout>